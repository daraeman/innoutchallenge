    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "connect-mongodb-session": "^1.3.0",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "daemonize2": "^0.4.2",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.15.4",
    "express-session": "^1.15.3",
    "fs.promised": "^3.0.0",
    "innout_locations": "git+https://github.com/daraeman/innout_locations.git",
    "json": "^9.0.6",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "moment-timezone": "^0.5.13",
    "mongodb": "^2.2.25",
