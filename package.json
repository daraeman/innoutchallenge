{
  "name": "innoutchallenge",
  "version": "1.0.0",
  "description": "#innoutChallenge Website",
  "main": "index.js",
  "private": true,
  "scripts": {
    "setup": "npm run start_database && node --trace-warnings server/app/init/setup.js; npm run stop_database",
    "start_server": "export NODE_ENV=production; export ENV_PATH='.env.production'; node server/server.js start http_server",
    "start_server_dev": "export NODE_ENV=dev; export ENV_PATH='.env.dev'; nodemon --trace-warnings server/app/http/server_daemon.js",
    "stop_server": "export NODE_ENV=production; export ENV_PATH='.env.production'; node server/server.js stop http_server",
    "build_client": "export NODE_ENV=production; export ENV_PATH='.env.production'; webpack --config webpack.config.js",
    "start_client_dev": "export NODE_ENV=dev; export ENV_PATH='.env.dev'; node_modules/.bin/webpack-dev-server",
    "start_database": "mongod --fork --config mongo.conf",
    "stop_database": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "pretest": "npm run start_database",
    "test": "export NODE_ENV=testing; export ENV_PATH='.env.testing'; node_modules/mocha/bin/mocha server/test/**.js; npm run stop_database"
  },
  "author": "David Rae",
  "license": "Apache-2.0",
