    "node-env-file": "^0.1.8",
    "node-tweet-stream": "^2.0.1",
    "oauth": "^0.9.15",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-rc.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "sinon": "^2.2.0",
    "style-loader": "^0.18.2",
    "timeago-react": "^1.2.2",
    "twitter": "^1.7.0",
    "twitter-request-queue-node": "^1.0.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.0",
    "winston": "^2.4.0",
    "word-to-number-node": "1.0.1"
  },
