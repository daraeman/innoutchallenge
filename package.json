{
  "name": "innoutchallenge",
  "version": "1.0.0",
  "description": "#innoutChallenge Website",
  "main": "index.js",
  "private": true,
  "scripts": {
    "setup": "node --trace-warnings server/app/setup.js;",
    "setup_dev": "node --trace-warnings server/app/setup.js;",
    "start_all": "npm run start_server && npm run start_fetch_tweets && npm run start_tweet_queue && npm run start_fetch_stores && npm run start_update_twitter_users",
    "start_server": "node server/server.js start http_server",
    "status_server": "node server/server.js status http_server",
    "start_server_dev": "nodemon --trace-warnings server/app/server_daemon.js",
    "stop_server": "node server/server.js stop http_server",
    "start_fetch_tweets": "node server/server.js start fetch_tweets",
    "status_fetch_tweets": "node server/server.js status fetch_tweets",
    "stop_fetch_tweets": "node server/server.js stop fetch_tweets",
    "start_tweet_queue": "node server/server.js start tweet_queue",
    "status_tweet_queue": "node server/server.js status tweet_queue",
    "stop_tweet_queue": "node server/server.js stop tweet_queue",
    "start_fetch_stores": "node server/server.js start fetch_stores",
    "status_fetch_stores": "node server/server.js status fetch_stores",
    "stop_fetch_stores": "node server/server.js stop fetch_stores",
    "start_update_twitter_users": "node server/server.js start update_twitter_users",
    "status_update_twitter_users": "node server/server.js status update_twitter_users",
    "stop_update_twitter_users": "node server/server.js stop update_twitter_users",
    "stop_all": "node server/server.js stop_all",
    "status_all": "node server/server.js status_all",
    "build_client": "webpack --config webpack.config.js",
    "start_client_dev": "node_modules/.bin/webpack-dev-server",
    "start_database": "if [ -z $(pidof mongod) ]; then mongod --config mongo.conf; else echo 'Mongod is already running'; fi;",
    "status_database": "if [ ! -z $(pidof mongod) ]; then echo 'Mongod is running'; else echo 'Mongod is not running'; fi;",
    "stop_database": "if [ ! -z $(pidof mongod) ]; then kill $(pidof mongod); else echo 'Mongod was not running'; fi;",
    "pretest": "npm run start_database",
    "test": "node_modules/mocha/bin/mocha server/test/**.js;"
  },
  "author": "David Rae",
  "license": "Apache-2.0",
