{
  "name": "innoutchallenge_backend",
  "version": "1.0.0",
  "description": "Backend for the #innoutChallenge",
  "main": "index.js",
  "private": true,
  "scripts": {
    "setup": "npm run startdb && node --trace-warnings app/init/setup.js; npm run stopdb",
    "startdb": "mongod --fork --config mongo.conf",
    "startdbdev": "mongod --config mongo.conf",
    "stopdb": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "start": "export NODE_ENV=production; export ENV_PATH='.env.production'; node app/http/server_daemon.js",
    "dev": "export NODE_ENV=dev; export ENV_PATH='.env.dev'; nodemon --trace-warnings app/http/server_daemon.js",
    "pretest": "npm run startdb",
    "test": "export NODE_ENV=testing; export ENV_PATH='.env.testing'; node_modules/mocha/bin/mocha test/**.js; npm run stopdb"
  },
  "author": "David Rae",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "connect-mongodb-session": "^1.3.0",
    "cors": "^2.8.4",
    "daemonize2": "^0.4.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "fs.promised": "^3.0.0",
    "innout_locations": "git+https://github.com/daraeman/innout_locations.git",
    "innoutchallenge_frontend": "git+https://github.com/daraeman/innoutchallenge_frontend.git",
    "json": "^9.0.6",
    "mongodb": "^2.2.25",
    "mongoose": "^4.9.7",
    "node-env-file": "^0.1.8",
    "node-tweet-stream": "^2.0.1",
    "oauth": "^0.9.15",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-rc.1",
    "sinon": "^2.2.0",
    "twitter": "^1.7.0",
    "twitter-request-queue-node": "^1.0.1",
    "word-to-number-node": "git+https://github.com/daraeman/word-to-number-node.git"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
